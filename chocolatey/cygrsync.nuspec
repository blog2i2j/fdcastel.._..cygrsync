    <docsUrl>https://rsync.samba.org/documentation.html</docsUrl>
    <mailingListUrl>https://rsync.samba.org/lists.html</mailingListUrl>
    <bugTrackerUrl>https://rsync.samba.org/bugzilla.html</bugTrackerUrl>
    <projectSourceUrl>https://git.samba.org/?p=rsync.git</projectSourceUrl>
    <tags>rsync</tags>
    <licenseUrl>http://www.gnu.org/licenses/old-licenses/gpl-2.0.html</licenseUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <iconUrl>http://upload.wikimedia.org/wikipedia/en/1/11/Newrsynclogo.png</iconUrl>
    <packageSourceUrl>https://github.com/fdcastel/cygrsync</packageSourceUrl>
  </metadata>
